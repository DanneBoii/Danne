	int value = 0;

	cout << "Enter an integer: ";
	cin >> value;
	cin.ignore();

	for (int i = 0; i < 1; i++)
	{
		if (value != 0)
		{
			cout << "Enter an integer: ";
			cin >> value;
			cin.ignore();
			i--;
		}
	}

	//Uppgift 5

	int upperLimit = 0;
	int lowerLimit = 0;
	int times = 0;

	cout << "Input the lower limit of the interval: ";
	cin >> lowerLimit;
	cin.ignore();
	cout << "Input the upper limit of the interval: ";
	cin >> upperLimit;
	cin.ignore();

	cout << lowerLimit << " ";

	while (lowerLimit < upperLimit)
	{
		lowerLimit++;
		cout << lowerLimit << " ";
	}

	//Uppgift 6

	int value = 0;
	int times = 0;
	int hardValue = 0;
	

	cout << "Input an integer: ";
	cin >> value;
	cin.ignore();
	
	hardValue = value;
	
	while (times < 10)
	{
		times++;
		value = hardValue * times;
		{
			cout << times << " * " << hardValue << " = " << value << endl;
		}
		value = 0;
	}
	
	//Uppgift 13
	
	srand((unsigned)time(0));
	int random1 = 0;
	int random2 = 0;
	int answer = 0;
	int guess = 0;
	int times = 0;
	double procent = 0.0;

	cout << "Time for you to show your math skills, type -1 to exit and see how good you did." << endl;

	while (guess != -1)
	{
		random1 = rand()%10 + 1;
		random2 = rand()%10 + 1;
		answer = random1 * random2;
		cout << random1 << " * " << random2 << " = ";
		cin >> guess;
		cin.ignore();

		if (guess == answer)
		{
			cout << "You gave the correct answer, Nice!" << endl;
			procent++;
			times++;
		}

		else
		{
			cout << "That answer is not correct, the correct answer is " << answer << endl;
			times++;
		}

	}
	times--;
	procent = (procent*100) / times;
	cout << "You guessed right " << procent << "% of the times";
