    double avr = 0.00f;
    cout << "Input an integer: ";
    cin >> v1;
    cout << "Input an integer: ";
    cin >> v2;
    cout << "Input an integer: ";
    cin >> v3;
    cout << "Input an integer: ";
    cin >> v4;
    cout << "Input an integer: ";
    cin >> v5;
    cin.ignore();
    avr = (v1 + v2 + v3 + v4 + v5) / 5;
    cout << "The average is: " << avr << endl;
    
    	//Uppgift 2 

	srand((unsigned)time(0));
	int upperLimit = 100;
	int lowerLimit = 1;
	int randomNr = 0;
	int nrOfTimes = 0;

	while (nrOfTimes < 10)
	{
		randomNr = rand() % (upperLimit - lowerLimit + 1) + lowerLimit;
		cout << randomNr << " ";
		nrOfTimes++;
	}
   
