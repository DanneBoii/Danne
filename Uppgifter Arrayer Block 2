#include <iostream>
#include <ctime>
#include <string>

using namespace std;

int main()
{
  //Uppgift 1
	srand((unsigned)time(0));
	int theArray[10] = {};
	int sum = 0;

	for (int i = 0; i < 10; i++)
	{
		theArray[i] = rand() % 99 + 1;
		sum += theArray[i];
		cout << theArray[i] << " ";
	}
	cout << "\nThe sum is: " << sum << endl;
